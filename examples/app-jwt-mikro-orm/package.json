{
  "name": "example-project",
  "version": "1.0.0",
  "description": "Express-mcs usage example with jwt and mikro-orm",
  "main": "src/index.ts",
  "author": "Nikita Nuykin",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@mikro-orm/cli": "5.1.3",
    "@mikro-orm/core": "5.1.3",
    "@mikro-orm/migrations": "5.1.3",
    "@mikro-orm/postgresql": "5.1.3",
    "body-parser": "1.20.0",
    "class-validator": "0.13.2",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "jsonwebtoken": "8.5.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/express": "4.17.13",
    "@types/jsonwebtoken": "8.5.8",
    "@types/uuid": "8.3.4",
    "concurrently": "7.1.0",
    "nodemon": "2.0.16",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "jest --run-in-band",
    "build": "yarn tsc",
    "start": "node ./dist/examples/app-jwt-mikro-orm/src/index.js",
    "dev": "concurrently \"yarn tsc --watch\" \"nodemon -q ./dist/examples/app-jwt-mikro-orm/src/index.js\"",
    "db:migration:new": "mikro-orm migration:create",
    "db:migrate": "mikro-orm migration:up"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
